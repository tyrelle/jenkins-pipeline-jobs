node ('local-mac'){
  stage 'Build'
      echo 'Reading config files'
      sh 'sleep 5s'


  stage 'Unit Tests'
    parallel (
      "eCatt Tests": {
        echo 'eCatt tests'
        git url: 'http://tyrelle.chetty@stash.standardbank.co.za:7990/scm/cbdev/jenkins-pipeline-jobs.git', branch: 'master'
        checkout scm
        outS = sh "groovy groovy-scripts/test.groovy"
        echo outS
      },
      "SOAP UI Tests": {
        echo 'SOAP UI tests'
        sh "sleep 10s"
      }
    )

  stage 'QA Tests'
    echo 'QA Tests'
    sh "sleep 4s"

  stage 'Release Transports'
    echo 'Releasing Transports'
    sh "sleep 2s"

  stage 'L1 Approval'
    input 'Give L1 Approvals?'
    sh "sleep 2s"

  stage 'L2 Approval'
    input 'Give L2 Approvals?'
    sh "sleep 2s"

  stage 'Deployed to SIT1'
    echo 'Transports moving to SIT1'
    sh "sleep 2s"

  stage 'QA Tests'
    echo 'QA Tests'
    sh "sleep 4s"
}
